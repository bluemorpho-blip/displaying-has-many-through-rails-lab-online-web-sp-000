<h1>Doctor#Show</h1>

<h2><%= @doctor.name %></h2>
<h2>Department: <%= @doctor.department %></h2>

<h2>Appointments:</h2>
<ul>
  <%= @doctor.appointments.each do |appt| %>
    <li><%= link_to appt.patient.name, patient_path(appt.patient) %></li>
    <li><%= appt.appointment_datetime %></li>
  <% end %>
</ul>

